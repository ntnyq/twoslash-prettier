{
  "name": "twoslash-prettier",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.4.1",
  "description": "Prettier-backed twoslash runner.",
  "keywords": [
    "prettier",
    "twoslash"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/twoslash-prettier#readme",
  "repository": "ntnyq/twoslash-prettier",
  "bugs": {
    "url": "https://github.com/ntnyq/twoslash-prettier/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "workers"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "unbuild --stub",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "prettier-linter-helpers": "^1.0.0",
    "synckit": "^0.9.2",
    "twoslash-protocol": "^0.2.12"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^4.0.0-beta.8",
    "@ntnyq/prettier-config": "^2.0.0",
    "@types/node": "^22.13.4",
    "@types/prettier-linter-helpers": "^1.0.4",
    "@vitest/coverage-v8": "^3.0.6",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.5.1",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "catalog:",
    "vitest": "^3.0.6"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  },
  "resolutions": {
    "@shikijs/core": "^3",
    "@shikijs/markdown-it": "^3",
    "@shikijs/rehype": "^3",
    "@shikijs/transformers": "^3",
    "@shikijs/types": "^3",
    "esbuild": "^0.25.0",
    "shiki": "^3"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
