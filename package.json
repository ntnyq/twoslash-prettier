{
  "name": "twoslash-prettier",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@10.20.0",
  "description": "Prettier-backed twoslash runner.",
  "keywords": [
    "prettier",
    "twoslash"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/twoslash-prettier#readme",
  "repository": "ntnyq/twoslash-prettier",
  "bugs": {
    "url": "https://github.com/ntnyq/twoslash-prettier/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "workers"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "deploy": "run-s build docs:build",
    "dev": "tsdown --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "generate-differences": "^0.1.0",
    "synckit": "^0.11.11",
    "twoslash-protocol": "^0.3.4"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^5.7.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.10.0",
    "bumpp": "^10.3.1",
    "eslint": "^9.39.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "pathe": "^2.0.3",
    "prettier": "^3.6.2",
    "show-invisibles": "^0.0.1",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.15.12",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vitest": "^4.0.6"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
