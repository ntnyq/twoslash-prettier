{
  "name": "twoslash-prettier",
  "type": "module",
  "version": "0.0.4",
  "packageManager": "pnpm@10.28.2",
  "description": "Prettier-backed twoslash runner.",
  "keywords": [
    "prettier",
    "twoslash"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://github.com/ntnyq/twoslash-prettier#readme",
  "repository": "ntnyq/twoslash-prettier",
  "bugs": {
    "url": "https://github.com/ntnyq/twoslash-prettier/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist",
    "workers"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "deploy": "run-s build docs:build",
    "dev": "tsdown --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s format:check lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "prettier": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "prettier": {
      "optional": true
    }
  },
  "dependencies": {
    "generate-differences": "^0.1.1",
    "synckit": "^0.11.12",
    "twoslash-protocol": "^0.3.6"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^6.0.0-beta.6",
    "@types/node": "^25.0.10",
    "@typescript/native-preview": "^7.0.0-dev.20260127.1",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.27.0",
    "pathe": "^2.0.3",
    "prettier": "^3.8.1",
    "show-invisibles": "^0.0.2",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.20.1",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.10",
    "vitest": "^4.0.18"
  },
  "nano-staged": {
    "*.{js,ts,mjs,cjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*": "oxfmt --no-error-on-unmatched-pattern"
  }
}
